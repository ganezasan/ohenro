ApiMapper=function(){},ApiMapper.prototype.apiEndpoint="http://aoyama.cheek-it.com/api",ApiMapper.prototype.accessApi=function(e,t,n,r,i){if(Ti.Network.online==0)return!1;var s=Ti.Network.createHTTPClient();return s.open(e,t),s.onload=r,s.onerror=i,s.send(n),!0},ApiMapper.prototype.spotAllApi=function(e,t){return this.accessApi("GET",this.apiEndpoint+"/spot/all.json",{},e,t)},ApiMapper.prototype.spotMyApi=function(e,t,n){return this.accessApi("GET",this.apiEndpoint+"/spot/my.json?token="+e,{},t,n)},ApiMapper.prototype.spotcheckinApi=function(e,t,n,r,i){return this.accessApi("POST",this.apiEndpoint+"/spot/checkin.json",{token:e,spot_id:t,comment:n},r,i)},ApiMapper.prototype.userregisterApi=function(e,t,n,r,i,s){return this.accessApi("POST",this.apiEndpoint+"/user/register.json",{name:e,social_type:t,social_token:n,social_secret:Ti.Utils.md5HexDigest("cheekit"+r)},i,s)},ApiMapper.prototype.usermyApi=function(e,t,n){return this.accessApi("GET",this.apiEndpoint+"/user/my.json?token="+e,{},t,n)},ApiMapper.prototype.usernotification=function(e,t,n,r,i,s,o){return this.accessApi("POST",this.apiEndpoint+"/user/notification.json",{token:e,social_type:t,social_token:n,social_secret:r,post:i},s,o)},exports.ApiMapper=ApiMapper