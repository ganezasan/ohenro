function S4(){return((1+Math.random())*65536|0).toString(16).substring(1)}function guid(){return S4()+S4()+"-"+S4()+"-"+S4()+"-"+S4()+"-"+S4()+S4()+S4()}function InitAdapter(e){throw"localStorage persistence supported only with MobileWeb."}function Sync(e,t,n){function r(e){localStorage.setItem(i,JSON.stringify(e))}var i=e.config.adapter.collection_name,s=e.config.data,o=null;switch(t){case"create":e.id||(e.id=guid(),e.set(e.idAttribute,e.id)),s[e.id]=e,r(s),o=e.toJSON();break;case"read":var u=localStorage.getItem(i),a=u&&JSON.parse(u)||{},f=0;for(var l in a){var c=new e.config.Model(a[l]);e.models.push(c),f++}e.length=f,f===1?o=e.models[0]:o=e.models;break;case"update":s[e.id]=e,r(s),o=e.toJSON();break;case"delete":delete s[e.id],r(s),o=e.toJSON()}o?(_.isFunction(n.success)&&n.success(o),t==="read"&&e.trigger("fetch")):_.isFunction(n.error)&&n.error("Record not found")}var _=require("alloy/underscore")._;module.exports.sync=Sync,module.exports.beforeModelCreate=function(e){return e=e||{},e.data={},InitAdapter(e),e},module.exports.afterModelCreate=function(e){return e=e||{},e.prototype.config.Model=e,e}